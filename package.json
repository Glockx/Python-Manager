{
  "name": "python-manager",
  "version": "1.1.6",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:types": "tsc --emitDeclarationOnly",
    "build": "bun run build.ts",
    "prepublishOnly": "bun run build && bun run build:types",
    "test": "jest",
    "docs": "bun typedoc",
    "updateDocs": "git subtree push --prefix docs origin gh-pages"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typedoc": "^0.27.7",
    "typedoc-material-theme": "^1.3.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "sudo-prompt": "^9.2.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Glockx/Python-Manager.git"
  },
  "license": "MIT"
}
